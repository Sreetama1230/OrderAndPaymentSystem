input ProductRequest{
name:String
description:String
price:Float
stockQuantity:Int
active:Boolean
tagRequest:[TagRequest]
}

input TagRequest{
name:String
}

type ProductResponse{
id:Int
name:String
description:String
price:Float
stockQuantity:Int
active:Boolean
tagResponse:[TagResponse]
}

type TagResponse{
id:Int
name:String
}

type Mutation{
createProduct(prodReq : ProductRequest) : ProductResponse
}

type Query{
findByProductName(name : String) : ProductResponse
}
